            var fromAddress = new MailAddress("no-reply@rndc-usa.com", "SSIS Package Notificaiton");
            var toAddress = new MailAddress("ratnakar.mada@rndc-usa.com", "");

            var smtp = new SmtpClient
            {
                Host = "relay1.rndc-usa.com",
                DeliveryMethod = SmtpDeliveryMethod.Network,
                UseDefaultCredentials = false,
                Timeout = 20000
            };

            MailMessage mailMsg = new MailMessage();
            mailMsg.IsBodyHtml = true;
            mailMsg.Subject = "SalesQuota4-Quota Brand Source job error";
            mailMsg.Body = "Package failed. Please contact IT Deliver-Commercial Team.<br>" + Dts.Variables["ErrorDescription"].Value.ToString();
            mailMsg.From = new MailAddress(fromAddress.ToString());
            mailMsg.To.Add(toAddress);
            //mailMsg.To.Add("vasantha.gutha@rndc-usa.com");
            //mailMsg.To.Add("weijie.xu@rndc-usa.com");
            //mailMsg.To.Add("Naresh.pasula@rndc-usa.com");

            {
                smtp.Send(mailMsg);
            }



            Dts.TaskResult = (int)ScriptResults.Success;
		}

